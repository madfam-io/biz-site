{
  "files": [
    {
      "path": "./apps/web/.next/static/**/*.js",
      "maxSize": "200kB",
      "compression": "gzip"
    },
    {
      "path": "./apps/web/.next/static/chunks/pages/**/*.js",
      "maxSize": "100kB",
      "compression": "gzip"
    },
    {
      "path": "./apps/web/.next/static/chunks/main-*.js",
      "maxSize": "30kB",
      "compression": "gzip"
    },
    {
      "path": "./apps/web/.next/static/chunks/webpack-*.js",
      "maxSize": "5kB",
      "compression": "gzip"
    },
    {
      "path": "./apps/web/.next/static/chunks/framework-*.js",
      "maxSize": "45kB",
      "compression": "gzip"
    },
    {
      "path": "./apps/web/.next/static/css/**/*.css",
      "maxSize": "20kB",
      "compression": "gzip"
    }
  ],
  "defaultCompression": "gzip",
  "normalizeFilenames": "^.+?-(\\w{20})\\.\\w+$",
  "ci": {
    "repoBranchBase": "main",
    "trackBranches": ["main", "develop"],
    "gitHubAccessToken": "$BUNDLEWATCH_GITHUB_TOKEN"
  }
}
